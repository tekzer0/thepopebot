#!/usr/bin/env node                                                             
                                                                                
                                                                                
                                                                                
// List all LLM-exposed credential key names (AGENT_LLM_*) without values       
                                                                                
const prefix = 'AGENT_LLM_';                                                    
                                                                                
                                                                                
                                                                                
// Collect matching environment variable keys                                   
                                                                                
const keys = Object.keys(process.env)                                           
                                                                                
